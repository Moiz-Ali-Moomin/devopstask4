# provide your certificate and key paths 
# used minikube as a node server

apiVersion: v1
kind: Config

clusters:
- cluster:
    server: https://192.168.99.101:8443
    certificate-authority: /root/Desktop/k8sconfig/ca.crt
  name: mycluster

contexts:
- context:
    cluster: mycluster
    user: moiz

users: 
- name: moiz
  user:
    client-key: /root/Desktop/k8sconfig/client.key
    client-certificate: /root/Desktop/k8sconfig/client.crt
